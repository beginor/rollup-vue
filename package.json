{
  "name": "rollup-starter",
  "version": "1.0.0",
  "description": "rollup starter kit",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run rollup && npm run copy-libs",
    "build-app": "rm -rf dist/main.* dist/chunks && npm run rollup",
    "clean": "rm -rf dist && mkdir dist",
    "copy-libs": "./copy-libs.sh",
    "browser-sync": "browser-sync start --config bs-config.js",
    "rollup": "rollup -c rollup.config.js",
    "rollup-watch": "npm run rollup -- --watch",
    "dev": "concurrently 'npm run browser-sync' 'npm run rollup-watch'",
    "start": "npm run clean && npm run copy-libs && npm run dev",
    "test": "echo test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/rollup-starter.git"
  },
  "keywords": [
    "rollup",
    "starter"
  ],
  "author": "beginor <beginor@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beginor/rollup-starter/issues"
  },
  "homepage": "https://github.com/beginor/rollup-starter#readme",
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "tslib": "^2.3.0",
    "vue": "^3.2.1",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.0",
    "eslint": "^7.32.0",
    "@typescript-eslint/parser": "^4.29.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "rollup": "^2.56.2",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.37.5",
    "typescript": "^4.3.5",
    "rollup-plugin-vue": "^6.0.0",
    "@vue/compiler-sfc": "^3.2.1",
    "eslint-plugin-vue": "^7.16.0"
  }
}
